@model Locador

@{
    ViewData["Title"] = "Detalhes do Locador";
}
@* TODO: fazer a página de Detalhes do locador *@
<h2>@ViewData["Title"]</h2>
<div class="row">
    <div class="col-md-6">
        <dl>
            <dt>Nome</dt>
            <dd>@Model.Nome</dd>
            <dt>Apelido</dt>
            <dd>@Model.Apelido</dd>
            <dt>Telefone</dt>
            <dd>@Model.PhoneNumber</dd>
        </dl>
    </div>
</div>
<div class="row">
    <div class="col-md-4">
        @* adicionar um botão para voltar à página de onde veio, em Areas/Identity/Pages/Account/Manage/GestaoLocadores *@
        <a class="btn btn-sm btn-primary" asp-page="/Identity/Pages/Account/Manage/GestaoLocadores">Voltar</a>
    </div>
</div>